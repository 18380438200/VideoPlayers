<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.videoplayers">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".videoview.VideoViewActivity" />
        <activity android:name=".mediaplayer.MediaPlayerActivity" />

        <receiver android:name=".mediaplayer.PlayVideo$ProgressBroadCast" />

        <activity android:name=".vitamioplayer.VitamioPlayerActivity" />
        <activity android:name=".jcplayer.JcPlayerActivity" />
        <activity android:name=".ijkplayer.IjkPlayerActivity"></activity>
    </application>

</manifest>